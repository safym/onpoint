{
  "name": "onpoint",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.22",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "npm-run-all -p watch:css start:js",
    "build": "npm-run-all build:css build:js",
    "start:js": "react-scripts start",
    "build:js": "react-scripts build",
    "build:css": "postcss src/assets/styles/style.css -o src/assets/styles/post.css",
    "watch:css": "postcss src/assets/styles/style.css -o src/assets/styles/post.css -w",
    "eject": "react-scripts eject",
    "prettier": "npx prettier --write .",
    "lint": "eslint -c .eslintrc --ext .js,.jsx .",
    "lint:fix": "npm run lint -- --fix",
    "babel-build": "npx babel src --out-dir lib"
  },
  "browserslist": "last 3 versions",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "autoprefixer": "^10.4.14",
    "gh-pages": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0",
    "postcss-nested": "^6.0.1",
    "prettier": "2.8.7"
  }
}
